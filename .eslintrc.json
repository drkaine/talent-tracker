{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  